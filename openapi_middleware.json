                              }
                            }
                          }
                        },
                        "tierwohl": {
                          "type": "object",
                          "properties": {
                            "tierwohlScore": { "type": "integer" },
                            "tierhaltung": {
                              "type": "object",
                              "additionalProperties": {
                                "type": "integer",
                                "minimum": 0,
                                "maximum": 100
                              }
                            }
                          }
                        },
                        "umwelt": {
                          "type": "object",
